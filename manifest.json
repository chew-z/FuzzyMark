{
    "author": "chew-z",
    "manifest_version": 2,
    "name": "Fuzzy Bookmarks",
    "offline_enabled": true,
    "description": "Fuzzy search your bookmarks",
    "version": "1.7.6",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "html/popup.html",
        "default_title": "Fuzzy Bookmarks"
    },
    "icons": { 
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "permissions": [
        "bookmarks"
    ]
}
